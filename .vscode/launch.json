	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "test",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\node_modules\\jest\\bin\\jest.js"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "eslint",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\node_modules\\eslint\\bin\\eslint.js",
			"args": [
				"src/**/*.*"
			]
		}
	]
}